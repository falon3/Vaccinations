<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.vaccinations">
    <class name="Manufacturer" table="vac_manufacturers">
        <id name="id" type="int" column="manufacturer_id" unsaved-value="0">
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String" column="name" not-null="true" length="255" />
    </class>
</hibernate-mapping>